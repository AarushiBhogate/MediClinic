@model List<MediClinic.Models.PhysicianPrescrip>
@{
    Layout = "~/Views/Shared/_PatientLayout.cshtml";
}


<h2>My Prescriptions</h2>

<table class="table table-bordered">
    <thead>
        <tr>
            <th>ID</th>
            <th>Drug</th>
            <th>Dosage</th>
            
        </tr>
    </thead>

    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.PrescriptionId</td>
                <td>@item.DrugId</td>
                <td>@item.Dosage</td>
                
            </tr>
        }
    </tbody>
</table>


